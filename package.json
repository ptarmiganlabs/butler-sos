        "esbuild": "^0.23.1",
        "eslint": "^9.9.1",
        "eslint-config-prettier": "^9.1.0",
        "eslint-formatter-table": "^7.32.1",
        "eslint-plugin-prettier": "^5.2.1",
        "globals": "^15.9.0",
        "prettier": "^3.3.3",
        "snyk": "^1.1292.4"
    },
    "pkg": {
        "assets": [
            "node_modules/axios/**/*",
            "node_modules/open/xdg-open",
            "static/**/*",
            "package.json"
        ],
        "scripts": [
            "node_modules/enigma.js/**/*.json",
            "node_modules/js-yaml/**/*.js"
        ]
    }
}
